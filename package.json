{
  "name": "custom-server-express",
  "version": "1.0.0",
  "scripts": {
    "preprod": "npm run build",
    "prod": "NODE_ENV=production node server/server.js",
    "build": "next build",
    "start": "node server/server.js",
    "docker": "docker run -it -v ${PWD}:/usr/src/app -v /usr/src/app/node_modules -p 3000:3000 --rm react-docker",
    "docker-prod": "docker run -it -v ${PWD}:/usr/src/app -v /usr/src/app/node_modules -p 3000:3000 --rm react-docker-prod",
    "docker-build": "docker build -t react-docker .",
    "docker-build-prod": "docker build -t react-docker-prod . -f prod.Dockerfile"
  },
  "dependencies": {
    "express": "^4.14.0",
    "next": "^6.1.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
